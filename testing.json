          "event": [
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "// Generate unique event name and future dates",
                  "const timestamp = Date.now();",
                  "pm.environment.set('uniqueEventName', 'Company Event ' + timestamp);",
                  "",
                  "// Generate future dates (30 days from now) in UTC",
                  "const now = new Date();",
                  "const startDate = new Date(now.getTime() + (30 * 24 * 60 * 60 * 1000)); // 30 days from now",
                  "startDate.setUTCHours(9, 0, 0, 0); // 9 AM UTC",
                  "",
                  "const endDate = new Date(startDate.getTime() + (8 * 60 * 60 * 1000)); // 8 hours later",
                  "",
                  "pm.environment.set('eventStartDate', startDate.toISOString());",
                  "pm.environment.set('eventEndDate', endDate.toISOString());",
                  "",
                  "console.log('Generated event dates:');",
                  "console.log('Start:', startDate.toISOString());",
                  "console.log('End:', endDate.toISOString());"
                ],
                "type": "text/javascript"
              }
            },
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status code is 201', function () {",
